{
    "presets": [
        ["@babel/preset-env", { "loose": true, "useBuiltIns": "entry" }]
    ],
    "plugins": [
        ["@babel/proposal-class-properties", { "loose": true }],
        "add-module-exports"
    ],
    "env": {
        "development": {
            "sourceMaps": "inline"
        }
    }
}
